{ 
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "client/$1"
        }
    ],
    "rewrites":[
        {
            "source":"",
            "destination":"https://server-proxy-backend.vercel.app/api/jokes"
        }
    ]
}
