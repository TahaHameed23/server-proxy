{ 
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }]
,
    "rewrites":[
        {
            "source":"/api/jokes",
            "destination":"https://server-proxy-backend.vercel.app/api/jokes"
        }
    ]
}
